{
  "name": "ama-eng",
  "version": "0.0.138",
  "description": "amaEng",
  "main": "index.ts",
  "scripts": {
    "start": "webpack serve --mode development",
    "startIsolated": "cross-env ISOLATED=true webpack serve --mode development --progress",
    "build": "rimraf dist & webpack --mode production --progress",
    "fastBuild": "set FAST=true&&webpack --mode production --progress",
    "patch": "npm version patch",
    "patch&Build": "npm version patch && npm run build",
    "test": "jest",
    "test:watch": "jest  --watch",
    "test:coverage": "jest  --coverage",
    "eslint": "eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VolodymyrKalynii/ama-eng.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VolodymyrKalynii/ama-eng/issues"
  },
  "homepage": "https://github.com/VolodymyrKalynii/ama-eng#readme",
  "browserslist": [
    "last 20 version",
    "> 1%"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/file-transformer.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coverageReporters": [
      "html"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ],
    "moduleNameMapper": {
      "@custom-types": "<rootDir>/src/custom-types",
      "^@lib/(.*)": "<rootDir>/src/lib/$1",
      "^_assets/(.*)": "<rootDir>/assets/$1",
      "^@store(.*)$": "<rootDir>/src/store/$1",
      "^@custom-types(.*)$": "<rootDir>/src/custom-types/$1",
      "^@global-config/(.*)": "<rootDir>/src/config/$1",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js"
    },
    "globals": {
      "window": {
        "location": {}
      }
    }
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.177",
    "@types/rc-tooltip": "^3.7.6",
    "@types/react": "^17.0.37",
    "@types/react-dnd-multi-backend": "^6.0.1",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.20",
    "@types/react-transition-group": "^4.4.4",
    "@types/styled-components": "^5.1.18",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.4.2",
    "html-webpack-plugin": "^5.5.0",
    "ip": "^1.1.5",
    "jest": "^27.3.1",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.0.1",
    "react-refresh": "^0.11.0",
    "react-refresh-typescript": "^2.0.3",
    "sass": "^1.43.5",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "thread-loader": "^3.0.4",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-xor": "^1.0.8",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
